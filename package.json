{
  "private": true,
  "name": "sanabutton.github.io",
  "scripts": {
    "start": "bundle exec jekyll serve",
    "test": "run-p lint",
    "build": "bundle exec jekyll build",
    "watch": "bundle exec jekyll build --watch",
    "lint": "prettier -l \"**/*\"",
    "fmt": "run-s format",
    "format": "prettier --write \"**/*\""
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2"
  }
}
