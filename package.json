{
  "private": true,
  "name": "sanabutton.github.io",
  "scripts": {
    "start": "bundle exec jekyll serve",
    "test": "run-p lint",
    "build": "bundle exec jekyll build",
    "watch": "bundle exec jekyll build --watch",
    "lint": "prettier -u -l \"**/*\"",
    "fmt": "run-s format",
    "format": "prettier -u -w \"**/*\""
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5"
  }
}
